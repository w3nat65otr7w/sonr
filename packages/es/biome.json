{
  "$schema": "https://biomejs.dev/schemas/2.1.4/schema.json",
  "extends": ["../../biome.json"],
  "linter": {
    "rules": {
      "suspicious": {
        "noExplicitAny": "off"
      },
      "correctness": {
        "noUnusedVariables": "warn"
      },
      "complexity": {
        "noStaticOnlyClass": "off"
      },
      "style": {
        "useTemplate": "warn",
        "noUnusedTemplateLiteral": "warn"
      }
    }
  }
}
