@go.Package { name = "github.com/onsonr/sonr/pkg/dwn" }

module dwn

import "package://pkg.pkl-lang.org/pkl-go/pkl.golang@0.5.0#/go.pkl"

typealias JSON = String

class Config {
  ipfs: IPFS
  sonr: Sonr
  motr: Motr
  schema: Schema
  proxyUrl: String
}

class IPFS {
  apiUrl: String
  gatewayUrl: String
}

class Motr {
  keyshare: JSON
  address: String
  origin: String
}

class Schema {
  version: Int
  account: String
  asset: String
  chain: String
  credential: String
  jwk: String
  grant: String
  keyshare: String
  profile: String
}

class Sonr {
  apiUrl: String
  grpcUrl: String
  rpcUrl: String
  webSocketUrl: String
  chainId: String
}

dwn : Config
