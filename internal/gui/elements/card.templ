package elements

func Card(size Size) templ.Component {
	return renderCard(size.CardAttributes())
}

templ renderCard(attrs templ.Attributes) {
	<div { attrs... }>
		<div class="container">
			<div class="row">
				<div class="col-md-12 space-3">
					{ children... }
				</div>
			</div>
		</div>
	</div>
}
