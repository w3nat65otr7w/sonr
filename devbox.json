{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/main/.schema/devbox.schema.json",
  "packages": [
    "gum@latest",
    "docker@latest",
    "go@1.24.4",
    "gcc@latest",
    "jq@latest",
    "trunk@latest",
    "mods@latest",
    "yq@latest"
  ],
  "env": {},
  "shell": {
    "init_hook": [
      "go mod download"
    ],
    "scripts": {
      "build": "make build-all",
      "test": "make test-all",
      "snapshot": "make snapshot",
      "release": "make release"
    }
  }
}
