{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.12.0/.schema/devbox.schema.json",
  "packages": [
    "bun@latest"
  ],
  "env": {
    "CLOUDFLARE_API_TOKEN": "$CLOUDFLARE_API_TOKEN",
    "GOPATH": "$HOME/go",
    "PATH": "$HOME/go/bin:$PATH",
    "TEMPL_EXPERIMENT": "rawgo",
    "CHAIN_ID": "sonr-testnet-1",
    "DENOM": "usnr",
    "KEYRING": "test",
    "MONIKER": "florence",
    "ENV": "$ENVIRONMENT"
  },
  "shell": {
    "scripts": {
      "build": [
        "make build"
      ]
    }
  }
}
